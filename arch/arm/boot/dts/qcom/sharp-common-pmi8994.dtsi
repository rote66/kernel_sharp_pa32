/*****************************************/
/*  pmi8994: qcom,pm8994 alias            */
/*****************************************/

&spmi_bus {
	
	qcom,pmi8994@2 {
		pmi8994_vadc: vadc@3100 {
		};
	};

	qcom,pmi8994@3 {
	};

	qcom,pm8994@0 {
	};
};

/*****************************************/
/*  pmi8994-gpios: qpnp-pin               */
/*****************************************/

&pmi8994_gpios {

	gpio@c000 { /* GPIO_01 */
		/* VFEL_RST */
		qcom,mode = <1>;			/* DIG_OUT */
		qcom,output-type = <0>;		/* CMOS */
		qcom,invert = <0>;			/* DISABLE */
		qcom,pull = <5>;			/* NO_PULL */
		qcom,vin-sel = <2>;			/* S4 */
		qcom,out-strength = <1>;	/* LOW */
		qcom,src-sel = <0>;			/* CONSTANT */
		qcom,master-en = <1>;		/* ENABLE */
		status = "okay";
	};

	gpio@c100 { /* GPIO_02 */
		/* SPPD_N */
		/* Qualcomm Settings */
	};

